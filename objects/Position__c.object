    <validationRules>
        <fullName>Date_Validation_rule</fullName>
        <active>true</active>
        <errorConditionFormula>Open_Date__c  &gt;=  Close_Date__c</errorConditionFormula>
        <errorMessage>If open date is equals or greater than closed date change the values in date field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_status</fullName>
        <active>true</active>
        <errorConditionFormula>AND
  (ISBLANK(Close_Date__c),
OR(ISPICKVAL(Status__c, &apos;Closed - Canceled&apos;) || 
   ISPICKVAL(Status__c, &apos;Closed - Filled&apos;)  || 
   ISPICKVAL(Status__c, &apos;Closed - Not Approved&apos;)) )</errorConditionFormula>
        <errorMessage>If the status is closed cancelled, closed date mandatory in terms creating record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Salary_Max</fullName>
        <active>true</active>
        <errorConditionFormula>Max_Pay__c &gt;= 1000000</errorConditionFormula>
        <errorMessage>Max Pay cannot be more than 1 million</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Salary_terms</fullName>
        <active>true</active>
        <errorConditionFormula>Min_Pay__c  &gt;=  Max_Pay__c</errorConditionFormula>
        <errorMessage>Min pay cannot be more than Max pay. Please the the data given</errorMessage>
    </validationRules>
